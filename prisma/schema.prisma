model bannerLayoutdb {
  id         Int    @id @default(autoincrement())
  title      String
  label      String
  image_path String
}

model purchaseUserData {
  id        Int    @id @default(autoincrement())
  name      String
  last_name String
  address   String
  details   String
  city      String
  state     String
  zip_code  String
  country   String
}

model paymentDataLayout {
  id              Int      @id @default(autoincrement())
  card_holder      String
  card_number      String
  expiration_date String
